{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "reenan-me",
  "main": "./dist/_worker.js/index.js",
  "compatibility_date": "2025-09-29",
  "compatibility_flags": ["nodejs_compat"],
  "routes": [
    {
      "pattern": "reenan.me",
      "custom_domain": true
    }
  ],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "observability": {
    "enabled": true
  }
}